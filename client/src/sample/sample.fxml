<GridPane alignment="center" hgap="10" prefHeight="455.0" prefWidth="658.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">


    <TableView fx:id="list" prefHeight="400.0" prefWidth="385.0" GridPane.columnIndex="1" GridPane.columnSpan="12" GridPane.rowIndex="2" />
   <ToolBar prefHeight="40.0" prefWidth="362.0" GridPane.columnIndex="1" GridPane.columnSpan="12" GridPane.rowIndex="1">
     <items>
         <Label fx:id="filters" prefHeight="18.0" prefWidth="50.0" text="Filters: " />
      
            
                <TextField fx:id="search" prefHeight="26.0" prefWidth="155.0" promptText="Search ..." />
      
                <MenuButton fx:id="severity" text="severity">
              <items>
                  <MenuItem text="TRIVIAL" />
                  <MenuItem text="IMPORTANT" />
                  <MenuItem text="URGENT" />
                  <MenuItem text="no severity" />
              </items>
          </MenuButton>
      
                <MenuButton fx:id="status" text="status">
              <items>
                  <MenuItem text="NOT STARTED" />
                  <MenuItem text="IN PROGRESS" />
                  <MenuItem text="DONE" />
                  <MenuItem text="no status" />
              </items>
          </MenuButton>
     </items>
   </ToolBar>
   <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" GridPane.columnIndex="17" GridPane.rowIndex="2">
      <children>
      
      
          <TextField fx:id="add_id" promptText="id" />
      
          <TextField fx:id="add_summary" promptText="summary" />
      
          <TextField fx:id="add_description" promptText="description" />
      
          <MenuButton fx:id="add_severity" text="severity">
              <items>
                  <MenuItem fx:id="TRIVIAL" text="TRIVIAL" />
                  <MenuItem text="IMPORTANT" />
                  <MenuItem text="URGENT" />
              </items>
          </MenuButton>
      
          <MenuButton fx:id="add_status" text="status">
              <items>
                  <MenuItem text="NOT STARTED" />
                  <MenuItem text="IN PROGRESS" />
                  <MenuItem text="DONE" />
              </items>
          </MenuButton>
      
          <Button fx:id="add" prefHeight="26.0" prefWidth="106.0" stylesheets="@style.css" text="add a new task" />
      
          <Label fx:id="error" prefHeight="18.0" prefWidth="50.0" text="default" visible="false" />
      </children>
      <GridPane.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </GridPane.margin>
   </VBox>
   <columnConstraints>
      <ColumnConstraints />
